{
  "name": "@anotherpit/nestjs-ws-routable-adapter",
  "version": "10.2.9",
  "description": "Extended version on NestJs' WsAdapter that supports route parameters",
  "main": "dist/index.js",
  "author": "anotherpit <anotherpit@gmail.com>",
  "license": "ISC",
  "keywords": [
    "nestjs",
    "websockets",
    "ws"
  ],
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build": "npm run clean && tsc -b ./tsconfig.app.json",
    "clean": "rm -rf ./dist",
    "lint": "eslint 'src/**/**.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepack": "npm run build",
    "test": "ts-mocha --type-check src/**/*.spec.ts",
    "test:dev": "ts-mocha -w --watch-files 'src/**/*.ts' --type-check src/**/*.spec.ts"
  },
  "dependencies": {
    "@nestjs/common": "10.2.9",
    "@nestjs/platform-ws": "10.2.9",
    "@nestjs/websockets": "10.2.9",
    "path-to-regexp": "3.2.0",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/platform-express": "10.2.9",
    "@nestjs/testing": "10.2.9",
    "@types/mocha": "10.0.4",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "prettier": "3.1.0",
    "superwstest": "^2.0.3",
    "ts-mocha": "^10.0.0",
    "typescript": "5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anotherpit/nestjs-ws-routable-adapter.git"
  },
  "files": [
    "dist"
  ]
}
